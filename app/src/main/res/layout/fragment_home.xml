<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_vertical"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_weight="1"

        android:baselineAligned="true"
        android:gravity="center_horizontal|center_vertical"
        android:orientation="horizontal">

        <include
            android:id="@+id/cardList1"
            layout="@layout/cardviews"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:layout_weight="1" />

        <include
            android:id="@+id/cardList2"
            layout="@layout/cardviews"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_weight="1"
        android:baselineAligned="true"
        android:gravity="center_horizontal|center_vertical"
        android:orientation="horizontal">

        <include
            android:id="@+id/cardList3"
            layout="@layout/cardviews"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:layout_weight="1" />

        <include
            android:id="@+id/cardList4"
            layout="@layout/cardviews"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_weight="1"
        android:baselineAligned="true"
        android:gravity="center_horizontal|center_vertical"
        android:orientation="horizontal">

        <include
            android:id="@+id/cardList5"
            layout="@layout/cardviews"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:layout_weight="1" />

        <include
            android:id="@+id/cardList6"
            layout="@layout/cardviews"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_weight="1"
        android:baselineAligned="true"
        android:gravity="center_horizontal|center_vertical"
        android:orientation="horizontal">

        <include
            android:id="@+id/cardList7"
            layout="@layout/cardviews"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:layout_weight="1" />

        <include
            android:id="@+id/cardList8"
            layout="@layout/cardviews"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_weight="1"
        android:baselineAligned="true"
        android:gravity="center_horizontal|center_vertical"
        android:orientation="horizontal">

        <include
            android:id="@+id/cardList9"
            layout="@layout/cardviews"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:layout_weight="1" />

        <include
            android:id="@+id/cardList10"
            layout="@layout/cardviews"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_weight="1" />

    </LinearLayout>

    <Button
        android:id="@+id/material_button"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="0"
        android:backgroundTint="#383838"
        android:backgroundTintMode="add"
        android:clickable="true"
        android:focusable="true"
        android:gravity="center_horizontal|center_vertical"
        android:maxHeight="48dp"
        android:minHeight="48dp"
        android:text="@string/notdone"
        android:textAlignment="center"
        android:textColor="@color/colorPrimaryDark"
        android:textIsSelectable="false"
        android:textSize="14sp"
        android:typeface="normal"
        android:visibility="visible" />
</LinearLayout>